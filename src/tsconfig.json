{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "rootDir": ".",
    "outDir": "../.tsc_build",
    "sourceMap": false
  },
  "filesGlob": [
    "**/*.ts",
    "typings/tsd.d.ts",
    "!jspm_packages/**/*.ts"
  ],
  "files": [
    "app/config.ts",
    "app/index.ts",
    "app/interfaces.ts",
    "app/run.ts",
    "components/api-client/index.ts",
    "components/api-client/interfaces.ts",
    "components/api-client/service.ts",
    "components/api-client/urls.ts",
    "components/session/index.ts",
    "components/session/interfaces.ts",
    "components/session/service.ts",
    "config/environments.ts",
    "config/interfaces.ts",
    "config/settings.ts",
    "index.ts",
    "sections/_base/controller.ts",
    "sections/_base/index.ts",
    "sections/search/controller.ts",
    "sections/search/index.ts",
    "sections/search/resolver.ts",
    "sections/search/route.ts",
    "sections/utils.ts",
    "typings/angular-material/angular-material.d.ts",
    "typings/angular-ui-router/angular-ui-router.d.ts",
    "typings/angularjs/angular-route.d.ts",
    "typings/angularjs/angular.d.ts",
    "typings/core-js/core-js.d.ts",
    "typings/gulp/gulp.d.ts",
    "typings/jquery/jquery.d.ts",
    "typings/node/node.d.ts",
    "typings/orchestrator/orchestrator.d.ts",
    "typings/q/Q.d.ts",
    "typings/tsd.d.ts"
  ],
  "exclude": []
}
